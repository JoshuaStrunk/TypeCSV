{
    "tableFormats": {
        "TestObject": {
            "headerMapping": [
                ["PropertyName", "PropertyType", "SpecialModifer"],
                ["PropertyDescription"]
            ]
        }
    },
    "defaultTableFormat": {
        "headerMapping": [
                ["PropertyName", "PropertyType", "SpecialModifer"]
            ]
    },
    "codeGenerators" : {
        "csharp": {
            "ext":".cs",
            "objectNameStyling": "CamelCase",
            "objectOpen":"public class {objectName} {",
            "objectClose": "}",
            "objectProperty": "public {propertyType} {propertyName};",
            "objectPropertyNameStyling": "camelCase",
            "typeMapping":{
                "Any": "string",    
                "String":"string",
                "Int": "int",
                "Float": "float"
            },
            "listType":"{propertyType}[]"
        },
        "typescript":{
            "ext":".d.ts",
            "objectNameStyling": "CamelCase",
            "objectOpen":"interface {objectName} {",
            "objectClose": "}",
            "objectProperty": "{propertyName} : {propertyType},",
            "objectPropertyNameStyling": "camelCase",
            "typeMapping":{
                "Any": "string",    
                "String":"string",
                "Int": "number",
                "Float": "number"
            },
            "listType":"{propertyType}[]"
        }
    }
}